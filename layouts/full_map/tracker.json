  "tracker_vertical": {
    "type": "container",
    "background": "#877373",
    "content": {
      "type": "dock",
      "min_height": 1000,
      "dropshadow": true,
      "content": [
        {
          "type": "dock",
          "dock": "bottom",
          "content": [
            {
              "type": "scroll",
              "dock": "bottom",
              "content": {
                "type": "layout",
                "dock": "bottom",
                "max_height": 480,
                "key": "items_section_vertical"
              }
            },
            {
              "type": "dock",
              "dock": "bottom",
              "v_alignment": "stretch",
              "max_height": 200,
              "min_height": 200,
              "max_width": "100%",
              "content": [
                {
                  "type": "group",
                  "header": "Pinned Locations",
                  "content": {
                    "type": "scroll",
                    "horizontal_scrollbar_visibility": "disabled",
                    "content": {
                      "type": "recentpins",
                      "style": "wrap",
                      "h_alignment": "stretch",
                      "v_alignment": "stretch",
                      "orientation": "horizontal",
                      "compact": true
                    }
                  }
                }
              ]
            }
          ]
        },
        {
          "type": "layout",
          "dock": "top",
          "key": "map_section"
        }
      ]
    }
  },
  "tracker_horizontal": {
